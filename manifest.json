{
  "manifest_version": 2,
  "name": "Home Manager Options Search Engine",
  "description": "Adds Home Manager options search engine",
  "version": "1.0",
  "author": "drymarchon",
  "homepage_url": "https://github.com/drymarchonshaun/firefox-hmopts-extension",
  "developer": {
    "name": "drymarchon",
    "url": "https://github.com/drymarchonshaun"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Home Manager Options",
      "search_url": "https://mipmip.github.io/home-manager-option-search/?query={searchTerms}",
      "keyword": "@hmopts",
      "favicon_url": "https://nixos.org/favicon.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
